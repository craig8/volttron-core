{
    "protected_topics": [
        "listener/stats",
        "devices/*",
        "historian_stats/*"
    ],
    "roles": {
        "edit_config_store": {
            "rpc_capabilities": [
                "config.store.add_config",
                "config.store.delete_config",
                "config.store.edit_config"
            ]
        },
        "admin": {
            "rpc_capabilities": [
                "*.*"
            ],
            "pubsub_capabilities": {
                "*": "pubsub"
            }
        },
        "test": {
            "pubsub_capabilities": {
                "test": "pubsub"
            }
        }
    },
    "user_groups": {
        "admin_users": {
            "identities": [
                "volttron.control",
                "config.store"
            ],
            "roles": [
                "admin"
            ]
        },
        "historian_users": {
            "identities": [
                "historian1"
            ],
            "rpc_capabilities": [
                "agent1.rpc1"
            ],
            "pubsub_capabilities": {
                "historian_stats/*": "publish"
            }
        }
    },
    "users": {
        "volttron.control": {
            "comments": "automatically added by platform on start"
        },
        "config.store": {
            "protected_rpcs": [
                "add_config",
                "edit_config",
                "delete_config"
            ],
            "comments": "automatically added by platform on start"
        },
        "listener1": {
            "pubsub_capabilities": {
                "listener/stats": "publish"
            },
            "roles": [
                {"edit_config_store": {
                        "id": "listener1"
                    }
                }
            ],
            "comments": "default entry added at install"
        },
        "platform.driver": {
            "pubsub_capabilities": {
                "devices/*": "publish"
            },
            "roles": [
                {"edit_config_store": {
                        "id": "platform.driver"
                    }
                }
            ]
        },
        "historian1": {
            "pubsub_capabilities": {
                "user_pubsub_topic": "pubsub"
            },
            "roles": [
                {"edit_config_store": {
                        "id": "historian1"
                    }
                }
            ]
        }
    }
}